     a - codigo da unidade 
     b - tipo de documento  -->
     <field action="add" tag="500">
       <pft>
         ,if ref(['phl_cnf']1,v221)='1' then
           ,ref(['phl_acv']l(['phl_acv'],'TIT='v2),'^a'v1,'^b'v26,'^c'mfn(1),),
         ,else
           ,ref(['phl_acv']l(['phl_acv'],'TIT='v2),'^a'v1,'^b'v5,'^c'mfn(1),),
